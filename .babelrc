{
  "presets": ["es2015", "react", "env"],
  "env": {
      "development": {
          "plugins": [
            "transform-runtime",
            "transform-object-rest-spread",
            ["react-transform", {
              "transforms": [{
                "transform" : "react-transform-hmr",
                "imports"   : ["react"],
                "locals"    : ["module"]
          }]
        }]
      ]
    }
  }
}
